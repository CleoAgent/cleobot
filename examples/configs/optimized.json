{
  "$schema": "https://cleobot.ai/schema/cleobot.json",
  "meta": {
    "description": "CleoBot with token optimization - model tiering and caching"
  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/.cleobot/workspace",
      "maxConcurrent": 4,
      "model": {
        "primary": "anthropic/claude-opus-4-5",
        "fallbacks": ["anthropic/claude-sonnet-4-5", "anthropic/claude-haiku-4-5"]
      },
      "models": {
        "anthropic/claude-opus-4-5": {
          "alias": "Opus",
          "params": {
            "cacheRetention": "long"
          }
        },
        "anthropic/claude-sonnet-4-5": {
          "alias": "Sonnet",
          "params": {
            "cacheRetention": "long"
          }
        },
        "anthropic/claude-haiku-4-5": {
          "alias": "Haiku",
          "params": {
            "cacheRetention": "short"
          }
        },
        "google/gemini-3-pro-preview": {
          "alias": "Gemini"
        },
        "moonshot/kimi-k2.5": {
          "alias": "Kimi"
        }
      },
      "subagents": {
        "maxConcurrent": 8,
        "model": "anthropic/claude-sonnet-4-5"
      },
      "heartbeat": {
        "every": "30m",
        "model": "anthropic/claude-haiku-4-5"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "contextPruning": {
        "mode": "cache-ttl",
        "ttl": "1h"
      }
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "token": "${CLEOBOT_GATEWAY_TOKEN}"
    }
  },
  "logging": {
    "redactSensitive": "tools"
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  }
}
